<head>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<div class="event-row row">
    <h3 class="text-center">You may be interested in...</h3>  
    <% events.forEach((event) => { %>
      <div class="col-sm-3 home-event-box">
        <img src="<%= event.image %>">
        <div class="event-info-wrapper">
          <a class="title" href="/home/<%= event._id %>">
            <strong><%= event.title %></strong>
          </a><br>
          by <span class="light-text"><%= event.creatorId.username%></span><br>
          <span class="light-text description"><%= event.description %></span>
        </div>
      </div>
    <%  }) %>
  </div>
  <button>
    <a href="/home/new">Nuevo evento</a>
    </button>
    <button>
      <a href="/auth/logout">Logout</a>
    </button>
    <button>
      <a href="/perfil">Editar perfil</a>
    </button>